/**index.wxss**/
page {
  width: 100%;
  background: #f6f6f6;
  box-sizing: border-box;
}
.header{
  width:100%;
  height: 300rpx;
  background-color: #ff3300;
}
swiper{
  position: relative;
  top: 0rpx;
  width: 730rpx;
  height: 300rpx;
  margin: 0 auto;
 
}
swiper image{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.highQualityMusics,
.mvList{
  width: 100%;
  height: auto;
}
.musicsTitle{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 80rpx;
  padding-left:30rpx;
  font-size: 14px;
}
.musicsContent{
  display:flex;
  flex-wrap: wrap;
  justify-content: space-evenly;
}
.music{
  border-radius:10rpx;
  width: 30%;
}
.music image{
  width: 100%;
  height:240rpx;
}
.music .music_title{
  font-size: 12px;

}
